  "description": "This project is for CMPT 353",
  "main": "server.js",
  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "docker-start": "node server.js",
    "client-install": "npm install  --prefix \"../client\"",
